{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current",
          "modules": "commonjs",
          "useBuiltIns": true,
          "debug": true
        },
        "forceAllTransforms": true
      }
    ],
    "stage-0",
    "stage-3",
    "next/babel",
    "react"
  ],
  "plugins": [
    "styled-jsx/babel",
    "transform-runtime",
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$"]
      }
    ]
  ]
}
