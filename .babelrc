{
  "presets": [
    "next/babel",
    "es2015",
    "stage-0",
    "stage-3",
    "react"
  ],
  "plugins": [
    "styled-jsx/babel",
    "transform-runtime",
    [
      "module-resolver",
      {
        "root": [
          "."
        ],
        "alias": {
          "styles": "./styles"
        },
        "cwd": "babelrc"
      }
    ],
    [
      "wrap-in-js",
      {
        "extensions": [
          "css$",
          "scss$"
        ]
      }
    ]
  ]
}